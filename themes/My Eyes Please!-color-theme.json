    "sideBarTitle.foreground": "#bbbbbb",
    "editorWidget.background": "#161414",
    "tab.inactiveBackground": "#0f1113",
    "tab.border": "#121416",
    "tab.hoverBackground": "#111416",
    "tab.activeForeground": "#5d616e",
    "editorGroupHeader.tabsBackground": "#0c0d0e",
    "scrollbarSlider.background": "#0c0d0e",
    "scrollbarSlider.hoverBackground": "#0b0b0c",
    "scrollbarSlider.activeBackground": "#0b0b0c",
    "activityBar.background": "#0c0d0e",
    "activityBar.foreground": "#323e4b",
    "statusBar.background": "#0c0d0e",
    "statusBar.foreground": "#1f252b",
    "editorBracketMatch.background": "#0c0d0e",
    "editorBracketMatch.border": "#1a2020",
    "editor.selectionBackground": "#1e2225",
    "editor.foldBackground": "#14161a",
    "list.activeSelectionBackground": "#090a0a",
    "list.inactiveSelectionBackground": "#090a0a",
    "list.hoverBackground": "#060707",
    "list.inactiveSelectionForeground": "#566c83",
    "list.hoverForeground": "#566c83",
    "list.activeSelectionForeground": "#566c83",
    "tree.indentGuidesStroke": "#21242b",
    "list.warningForeground": "#4a5664",
    "list.errorForeground": "#eb0c44",
    "input.background": "#0c0d0e",
    "input.foreground": "#566c83",
    "gitDecoration.ignoredResourceForeground": "#21242b",
    "editorGutter.modifiedBackground": "#243f61",
    "editorGutter.addedBackground": "#315e4f",
    "editorGutter.deletedBackground": "#612424",
    "editorError.foreground": "#eb0c44",
    "editorWarning.foreground": "#4bcf82",
    "foreground": "#3a424b"
